    "print(s4.strip(\"-\"))  #只在字符串前后查找\n",
    "\n",
    "# 切片 \n",
    "a3 = \"123456\"\n",
    "print(a3[1])   #python是从0开始数数的\n",
    "print(a3[1:3])"
